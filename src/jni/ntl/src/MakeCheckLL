
echo "Checking for working LL type"

cp ../include/NTL/have_LL_no.h ../include/NTL/have_LL.h

sh RemoveProg CheckLL
sh AndroidRemoveProg $3/CheckLL
echo $1 CheckLL.cpp CheckLLAux.cpp
$1 CheckLL.cpp CheckLLAux.cpp
echo $2 -o CheckLL CheckLL.o CheckLLAux.o
$2 -o CheckLL CheckLL.o CheckLLAux.o

adb push CheckLL $3/

if test -f CheckLL
then
   if adb shell $3/CheckLL
   then
      echo "have LL type"
      cp ../include/NTL/have_LL_yes.h ../include/NTL/have_LL.h
      sh RemoveProg CheckLL
      sh AndroidRemoveProg $3/CheckLL
      exit 0
   fi
fi

echo "DO NOT have LL type"
sh RemoveProg CheckLL
sh AndroidRemoveProg $3/CheckLL
exit 0

